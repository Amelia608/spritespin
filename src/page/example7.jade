extends layouts/example.jade
block content
  .spritespin
  script(type='text/javascript').
    $('.spritespin').spritespin({
      source: SpriteSpin.sourceArray('images/rad_zoom_{frame}.jpg', { frame: [1,34], digits: 3 }),
      width: 480,
      height: 480,
      sizeMode: 'fill',
      sense: -1,
      mods: ['dragDoc', 'ease', '360', 'blur'],
      blurCss: true
    });
